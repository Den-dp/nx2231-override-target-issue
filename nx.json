{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "production": [
      "default",
      "!{projectRoot}/**/*.Tests/**/*",
      "!{projectRoot}/**/bin/**/*",
      "!{projectRoot}/**/obj/**/*"
    ],
    "sharedGlobals": []
  },
  "plugins": [
    {
      "plugin": "@nx/dotnet",
      "options": {
        "build": false,
        "build:release": false,
        "publish": false,
        "pack": false,
        "watch": false,
        "run": false,
        "restore": false,
        "clean": false
      }
    }
  ]
}
